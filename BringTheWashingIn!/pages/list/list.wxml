<!--pages/list/list.wxml-->
<view class="date-items" wx:for="{{weeklyWeatherForcast}}" wx:key="weeklyWeatherForcast">
  <view class="date-wrapper">
    <view class="day">{{item.day}}</view>
    <view class="date">{{item.date}}</view>
  </view>
  <view class="temp">{{item.temp}}</view>
  <image class="weather-icon" src="{{item.weatherIconPath}}"></image>
</view>
